<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Mobile_Test_Execution_Suite" parallel="tests" thread-count="5">
    <listeners>
        <listener class-name="com.vitamojo.listeners.TestNGExecutionListener"/>
        <listener class-name="com.vitamojo.listeners.TestAllureListener"/>
<!--        <listener class-name="com.pricer.listeners.TestListener"/>-->
    </listeners>

    <test name="VitaMojo_Mobile_Test">
        <parameter name="dataID" value="VitaMojo_Mobile_Automation"/>
        <classes>
            <class name="com.vitamojo.mobile.Test_Scenarios_For_VitaMojo_E2E_Functionality">
                <methods>
<!--                    <include name="verify_LoginFailsWithInvalidPassword"/>-->
                    <include name="verify_LoginSuccessWithCorrectCredentials"/>
                    <include name="verify_HamburgerMenuIconDisplayed"/>
                    <include name="verify_StockQuantityDisplayed"/>
                    <include name="verify_UserLandOnStockQuantityPageSuccessfully"/>
                    <include name="verify_ProductCountDisplayedOnStockQuantityPage"/>
                    <include name="verify_VirtualBrandsDisplayed"/>
                    <include name="verify_UserLandOnVirtualBrandsPageSuccessfully"/>
                    <include name="verify_LogOutButtonDisplayed"/>
                    <include name="verify_LogOutAndLandOnLogInPage"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>