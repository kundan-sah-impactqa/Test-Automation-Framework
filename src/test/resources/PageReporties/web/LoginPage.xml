<objects>

	<object name="txtUsername">
		<web locatorType="xpath" locatorValue="//input[@id='txtUsername']" />
	</object>

	<object name="txtPassword">
		<web locatorType="xpath" locatorValue="//input[@type='password']" />
	</object>

	<object name="btnLogin">
		<web locatorType="xpath" locatorValue="//a[text()='Login']" />
	</object>

	<object name="lblInvalidUsernameAndOrPassword">
		<web locatorType="xpath" locatorValue="//div[@class='msgAlertPlaceHolder']" />
	</object>

</objects>