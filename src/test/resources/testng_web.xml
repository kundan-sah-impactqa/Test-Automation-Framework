<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Web_Test_Execution_Suite" parallel="classes" thread-count="1" data-provider-thread-count="1">
    <listeners>
        <listener class-name="com.vitamojo.listeners.TestNGExecutionListener"/>
        <!--        <listener class-name="com.pricer.listeners.TestListener"/>-->
    </listeners>

    <test name="Starbucks_Test">
        <!--
          To run on Chrome browser pass browser value as : "chrome"
          To run on Firefox browser pass browser value as : "firefox"
        -->
        <parameter name="browser" value="chrome."/>
        <parameter name="dataID" value="Starbucks_Web_Automation"/>
        <classes>

        <class name="com.vitamojo.web.Test_Scenarios_For_Login_Functionality">
                <methods>
                    <include name="verify_PricerApplicationOpensOnBrowserSuccessfully"/>
                </methods>
            </class>

        </classes>
    </test>

</suite>